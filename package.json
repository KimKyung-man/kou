{
  "name": "kou",
  "version": "0.0.0",
  "description": "A minimal language compiled into wasm bytecode",
  "main": "index.js",
  "bin": {
    "kou": "bin/kou"
  },
  "scripts": {
    "build": "tsc",
    "dev": "sane 'npm run format && npm run build' src",
    "prettier": "prettier --parser typescript --single-quote --trailing-comma all '{src,test}/**/*.ts'",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- -l",
    "test": "npm run check-format && npm run build && ts-node --no-cache --type-check test",
    "release": "npm run build && npm publish",
    "precommit": "npm run check-format",
    "prepush": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/utatti/kou.git"
  },
  "author": "Hyunje Jun <me@noraesae.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/utatti/kou/issues"
  },
  "homepage": "https://github.com/utatti/kou",
  "devDependencies": {
    "husky": "^0.14.3",
    "prettier": "^1.8.2",
    "sane": "^2.2.0",
    "ts-node": "^4.0.1",
    "typescript": "^2.6.1"
  },
  "dependencies": {
    "@typed/compose": "^1.0.0",
    "chalk": "^2.3.0",
    "previewable-iterator": "^0.1.1"
  }
}
